{
  "name": "d3-hilbert",
  "version": "1.0.0",
  "description": "D3 layout to visualize distance variables using a continuous Hilbert space-filling space.",
  "keywords": [
    "d3",
    "hilbert"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "unpkg": "dist/d3-hilbert.min.js",
  "main": "dist/d3-hilbert.common.js",
  "module": "dist/d3-hilbert.module.js",
  "homepage": "https://github.com/vasturiano/d3-hilbert",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-hilbert.git"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/d3-hilbert.js -o dist/d3-hilbert.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepublish": "npm run build && npm run minify && npm run build-module"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2",
    "rollup": "0.63",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.4.5"
  }
}
